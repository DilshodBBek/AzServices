{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\identity-app.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "StartsWith(RequestPath, '/Identity/Account') and (Level = 'Error')"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://10.10.1.45:5038",
        "Protocols": "Http1"
      },
      "Http2": {
        "Url": "https://localhost:7056",
        "Protocols": "Http2"
      }
    }
  }
}
